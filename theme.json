	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"radius": "4px"
				},
				"color": {
					"background": "var(--wp--preset--color--secondary)",
					"text": "var(--wp--preset--color--base)"
				}
			},
			"core/heading": {
				"spacing": {
					"margin": {
						"top": "20px",
						"bottom": "20px"
					}
				}
			},
			"core/post-title": {
				"spacing": {
					"margin": {
						"top": "20px",
						"bottom": "20px"
					}
				}
			},
			"core/post-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)",
					"fontWeight": "400",
					"lineHeight": "1.5"
				},
				"spacing": {
					"margin": {
						"left": "20px",
						"right": "20px"
					}
				}
			},
			"core/post-author": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)",
					"fontWeight": "400",
					"lineHeight": "1.5"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--primary)"
		},
		"elements": {
			"button": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--chivo)",
					"fontSize": "33px",
					"fontStyle": "normal",
					"fontWeight": "300"
				}
			},
			"caption": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--chivo)",
					"fontSize": "var(--wp--preset--font-size--regular)",
					"fontStyle": "normal",
					"fontWeight": "200"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)",
					"fontStyle": "normal",
					"fontWeight": "400",
					"lineHeight": "1.2"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "400",
					"lineHeight": "1.3"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "400",
					"lineHeight": "1.4"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--alfa-slab-one)",
					"fontStyle": "normal",
					"fontWeight": "400",
					"letterSpacing": "0.2px",
					"lineHeight": "1.5"
				}				
			},
			"link": {
				"color": {
					"text": "#333841"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--chivo)",
					"fontSize": "var(--wp--preset--font-size--regular)",
					"fontStyle": "normal",
					"fontWeight": "300"
				}
			}
		},
		"spacing": {
			"blockGap": "0px",
			"padding": {
				"bottom": "0px",
				"left": "0px",
				"right": "0px",
				"top": "0px"
			}
